{
  "AppDataServiceSettings": {
    "ServiceConfig": {
      "AppName": "hello-adminshell"
    }
  },
  "ServerSettings": {
    "ServerConfig": {
      "Hosting": {
        "Environment": "Development",
        "ContentPath": "Content",
        "Urls": [
          "http://+:5080"
        ]
      },
      "DefaultRoute": "/ui"
    },
    "UserInterfaceConfig": {
      "Title": "AdminShell Hello World",
      "BrandLogo": "/files/images/MyCompanyLogo.png",
      "Links": [
        {
          "Name": "AdminShell-UI",
          "Url": "/ui"
        }
      ]
    },
    "DiscoveryConfig": {
      "AutoDiscovery": false
    }
  },
  "RegistryClientSettings": {
    "ClientConfig": {
      "RequestConfig": {
        "RequestTimeout": 10000
      }
    },
    "RegistryConfig": {
      "RegistryUrl": "https://127.0.0.1:8443/registry"
    },
    "ProxyConfig": {
      "UseProxy": false,
      "ProxyAddress": "http://localhost:3128"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
