{
  "AppDataServiceSettings": {
    "ServiceConfig": {
      "AppName": "adminshell-registry"
    }
  },
  "ServerSettings": {
    "ServerConfig": {
      "Hosting": {
        "Environment": "Development",
        "ContentPath": "Content",
        "Urls": [
          "http://+:4999"
        ]
      },
      "DefaultRoute": "/ui",
      "PathBase": "/registry"
    },
    "UserInterfaceConfig": {
      "Title": "AdminShell Registry",
      "BrandLogo": "/images/MyCompanyLogo.png",
      "Links": [
        {
          "Name": "AdminShellRegistry-UI",
          "Url": "/registry/ui"
        }
      ]
    },
    "DiscoveryConfig": {
      "AutoDiscovery": false
    }
  }, 
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
